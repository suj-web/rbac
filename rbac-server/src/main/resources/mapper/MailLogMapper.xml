<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rbac.mapper.MailLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.rbac.pojo.MailLog">
        <id column="msg_id" property="msgId" />
        <result column="employee_id" property="employeeId" />
        <result column="status" property="status" />
        <result column="route_key" property="routeKey" />
        <result column="exchange" property="exchange" />
        <result column="count" property="count" />
        <result column="try_time" property="tryTime" />
        <result column="is_delete" property="isDelete" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        msg_id, employee_id, status, route_key, exchange, count, try_time, is_delete, gmt_create, gmt_modified
    </sql>

</mapper>
